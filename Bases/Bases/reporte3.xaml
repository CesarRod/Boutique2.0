<Window x:Class="Bases.reporte3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bases"
        mc:Ignorable="d"
        Title="Cantidad de Prendas por Tipo, Modelo y Proveedor" Height="356" Width="355">
    <Grid Margin="0,0,2,11">
        <Button x:Name="atras" Content="Atras" Margin="260,0,0,9" VerticalAlignment="Bottom" Click="Button_Click" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Width="75">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.646"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <DataGrid x:Name="lista" CanUserAddRows="False" SelectionMode="Single"  SelectionUnit="FullRow" Margin="10,10,10,0" Height="258" VerticalAlignment="Top"/>

    </Grid>
</Window>
