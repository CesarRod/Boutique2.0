<Window x:Class="Bases.InventarioBajas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bases"
        mc:Ignorable="d"
        Title="Eliminar articulo" Height="322" Width="369">
    <Grid>
        <Label Content="Modelo" HorizontalAlignment="Left" Height="28" Margin="10,10,0,0" VerticalAlignment="Top" Width="62"/>
        <TextBox x:Name="modelo" HorizontalAlignment="Left" Height="30" Margin="10,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="123"/>
        <DataGrid x:Name="lista" CanUserAddRows="False" Margin="10,95,10,96"/>
        <Button x:Name="borrar" Content="Borrar" HorizontalAlignment="Left" Height="21" Margin="10,244,0,0" VerticalAlignment="Top" Width="70" Click="borrarFunc"/>
        <Button x:Name="buscar" Content="Buscar" HorizontalAlignment="Left" Height="21" Margin="267,47,0,0" VerticalAlignment="Top" Width="78" Click="busqueda"/>
        <Button x:Name="atras" Content="Atras" HorizontalAlignment="Left" Height="21" Margin="275,244,0,0" VerticalAlignment="Top" Width="70" Click="atras_Click"/>

    </Grid>
</Window>
